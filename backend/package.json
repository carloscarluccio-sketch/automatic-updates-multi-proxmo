{
  "name": "multpanel-backend",
  "version": "2.0.0",
  "description": "Proxmox Multi-Tenant Platform - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:integration": "jest --testPathPattern=Controller.test.ts",
    "test:unit": "jest --testPathPattern=Service.test.ts",
    "test:e2e": "cd .. && npx playwright test"
  },
  "keywords": [
    "proxmox",
    "multi-tenant",
    "virtualization",
    "management"
  ],
  "author": "Carlos Carluccio",
  "license": "UNLICENSED",
